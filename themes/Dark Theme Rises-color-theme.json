{
	"name": "Dark Theme Rises",
	"type": "dark",
	
	"colors": {
		"editor.background": "#181c1f",
		"editor.foreground": "#eeffff",
		"editorError.foreground": "#db4213ad",
		"editor.lineHighlightBorder": "#ffffff00",
		"editor.lineHighlightBackground": "#101314b9",
		"editorLineNumber.foreground": "#323f49",
		"editorLineNumber.activeForeground": "#eeffff",
		"editorGroupHeader.tabsBackground": "#1c2125",

		"tab.inactiveBackground": "#1c2125",
		"tab.inactiveForeground": "#7b848d",
		"tab.unfocusedInactiveForeground": "#7b848d",
		"tab.activeForeground": "#62bbff",
		"tab.unfocusedActiveForeground": "#d8e5f0",
		"tab.activeBackground": "#181c1f",
		"tab.unfocusedActiveBackground": "#181c1f",
		"tab.unfocusedActiveBorder": "#181c1f",
		"tab.activeBorder": "#2994e6",

		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",
		// "activityBar.foreground": "#007acc",
		"sideBar.background": "#121416",
		"sideBar.foreground": "#787e8a",
		"sideBarSectionHeader.background" : "#181b1d",
		"sideBarSectionHeader.border" : "#121416",

		"sideBarSectionHeader.foreground":"#b5b7b9" ,

		"activityBar.background": "#0d0e0f",
	},
	"workbench.colorCustomizations": {

	},
	 "tokenColors": [
    {
      "scope": ["meta.embedded", "source.groovy.embedded", "meta.class"],
      "settings": {
        "foreground": "#40b48e"
      }
	},
	{
		"scope":["keyword.operator.arithmetic"],
		"settings":{
			"foreground": "#ffa600",
		}
	},
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#619947"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
	  "scope": [
		"entity.other.attribute-name",
		"meta.object-literal.key"
	  ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#569cd6"
      }
    },
    {
